{
  "extends": [
    "@parcel/config-default"
  ],
  "reporters": [
    "...",
    "parcel-reporter-clean-dist"
  ],
  "transformers": {
    "*.hbs": [
      "parcel-transformer-hbs"
    ]
  },
  "optimizers": {
    "js": {
      "terserOptions": {
        "mangle": {
          "reserved": [
            "os"
          ]
        }
      }
    }
  }
}
